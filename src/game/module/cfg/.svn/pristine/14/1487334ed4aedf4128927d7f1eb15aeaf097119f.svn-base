class StdDaily { 
    /** 字段名 */
    id: number;
    /** 描述 */
    desc: string;
    /** 类型（0：日常类，1：活动类） */
    type: number;
    /** 解锁描述 */
    conddesc: string;
    /** 解锁条件（0：等级限制，1：开服天数限制） */
    cond: any[] = [];
    /** 完成条件（id：0无限制） */
    target: any;
    /** 奖励（默认活跃度） */
    award: number;
    /** 跳转模块 */
    mod: any;
    /** 图片地址 */
    picture: string;
}