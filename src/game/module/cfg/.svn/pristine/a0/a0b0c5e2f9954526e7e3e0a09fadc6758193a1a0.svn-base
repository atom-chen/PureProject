class StdBuff { 
    /** BuffID */
    id: number;
    /** 名称 */
    name: string;
    /** 属性类型 */
    type: number;
    /** 属性值 */
    value: number;
    /** buff组 */
    group: number;
    /** 使用次数 */
    times: number;
    /** 间隔 */
    interval: number;
    /** 时间叠加 */
    timeoverlay: number;
    /** 属性叠加 */
    attroverlay: number;
    /** icon */
    icon: number;
    /** 保存 */
    issave: number;
    /** 是否要使用param2，param3 */
    param: number;
    /** 参数2 */
    param2: number;
    /** 参数3 */
    param3: number;
    /** 前端使用 */
    display: number;
    /** 是否忽视time限制，下线重登使用 */
    isignoretime: number;
}